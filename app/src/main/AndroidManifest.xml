<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CafeApp"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".view.ReservationActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".view.ReservationListActivity"
            android:exported="false"
            android:parentActivityName=".view.ReservationActivity" />
        <activity
            android:name=".view.ReservationDetailActivity"
            android:exported="false"
            android:parentActivityName=".view.ReservationListActivity" />
        <activity
            android:name=".view.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".view.AdminHomeActivity"
            android:exported="false" />
        <activity
            android:name=".view.ProductListActivity"
            android:exported="false" />
        <activity
            android:name=".view.TableListActivity"
            android:exported="true" />
        <activity
            android:name=".view.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".view.ForgotPasswordActivity"
            android:exported="false" />
    </application>

</manifest>